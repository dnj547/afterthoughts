


<h1>Sign it UP!</h1>
<%=form_for @user, controller:'users', action:'create' do |f|%>
<%=f.label :username%>
<%=f.text_field :username%>
<%=f.label :password%>
<%=f.password_field :password%>
<%=f.label :password_confirmation%>
<%=f.password_field :password_confirmation%>
<%=f.submit "Sign Up!"%>
<%end%>
